# .env.dev - Environment variables for LOCAL Docker development

# --- Django Settings ---
SECRET_KEY=django-insecure-a-local-dev-secret-key-for-ai4s
DEBUG=1
ALLOWED_HOSTS=localhost,127.0.0.1,nginx

# --- PostgreSQL Container Settings ---
SQL_DATABASE=ai4s_db
SQL_USER=ai4s_user
SQL_PASSWORD=a_strong_password_for_db

# --- Database URL for Django ---
DATABASE_URL=postgres://ai4s_user:a_strong_password_for_db@db:5432/ai4s_db